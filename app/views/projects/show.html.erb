<div class="page-header">
  <%= link_to projects_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_project_path(@project), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show project</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @project.title %></dd>

  <dt>Description:</dt>
  <dd><%= @project.description %></dd>

  <dt>Inspiration url:</dt>
  <dd><%= @project.inspiration_url %></dd>

  <dt>Proposed location:</dt>
  <dd><%= @project.proposed_location %></dd>

  <dt>Proposed date:</dt>
  <dd><%= @project.proposed_date %></dd>

  <dt>Created:</dt>
  <dd><%= @project.created_at %></dd>

</dl>
